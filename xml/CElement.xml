<?xml version='1.0' encoding='utf-8'?>
<class id="CElement" name="CElement" xmlns="http://jessevdk.github.com/cldoc/1.0">
  <brief>Element class</brief>
  <doc>This class definition holds, sets, and gets all of the radiative emission
information associated with a particular element. A new instance of <ref ref="CElement#CElement::CElement">CElement</ref>
is instantiated for each element in the configuration file when <ref ref="CRadiation#CRadiation">CRadiation</ref>
is instantiated.
</doc>
  <constructor id="CElement::CElement" name="CElement">
    <brief>Default constructor</brief>
    <argument id="CElement::CElement::iZ" name="iZ">
      <doc>atomic number of element</doc>
      <type builtin="yes" name="int" />
    </argument>
    <argument id="CElement::CElement::szRangesFilename" name="szRangesFilename">
      <doc>temperature and density ranges data filename</doc>
      <type builtin="yes" name="char" qualifier="*" />
    </argument>
    <argument id="CElement::CElement::szAbundFilename" name="szAbundFilename">
      <doc>element abundance data filename</doc>
      <type builtin="yes" name="char" qualifier="*" />
    </argument>
    <argument id="CElement::CElement::szEmissFilename" name="szEmissFilename">
      <doc>element emissivity data filename</doc>
      <type builtin="yes" name="char" qualifier="*" />
    </argument>
    <argument id="CElement::CElement::szRatesFilename" name="szRatesFilename">
      <doc>element ionization rates data filename</doc>
      <type builtin="yes" name="char" qualifier="*" />
    </argument>
    <argument id="CElement::CElement::szIonFracFilename" name="szIonFracFilename">
      <doc>element fractional ionization data filename</doc>
      <type builtin="yes" name="char" qualifier="*" />
    </argument>
    <argument id="CElement::CElement::doEmissCalc" name="doEmissCalc">
      <doc>if True, do emissivity calculation; set to False in IonPopSolver</doc>
      <type builtin="yes" name="bool" />
    </argument>
    <argument id="CElement::CElement::root" name="root">
      <doc>XML document tree root to radiation configuration file</doc>
      <type name="tinyxml2::XMLElement" qualifier="*" />
    </argument>
  </constructor>
